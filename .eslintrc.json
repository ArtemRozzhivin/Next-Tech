{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-undef": "warn",
    "@typescript-eslint/no-explicit-any": "warn",
    "linebreak-style": ["error", "unix"],
    "no-console": "off",
    "no-alert": "off",
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 4,
        "maxEOF": 1
      }
    ],
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "strict": ["off", "global"],
    "newline-before-return": "error"
  }
}
